{
  "compilerOptions": {
    "target": "es6",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./public" // Hasil kompilasi akan diletakkan di folder public
  },
  "include": ["src"]
}
